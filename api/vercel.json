{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "src/server.ts"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "access-control-allow-origin",
                    "value": "*"
                }
            ]
        }
    ]
}
